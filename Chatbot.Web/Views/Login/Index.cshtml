@using Chatbot.Model.User
@model UserSignRequest

<div class="login">
    <form id="form-login">
        <p>Đăng nhập</p>
        <div class="form-group">
            <input asp-for="UserName" />
        </div>
        <div class="form-group">
            <input asp-for="Password" />
        </div>
        <button type="submit">Đăng nhập</button>
    </form>
</div>